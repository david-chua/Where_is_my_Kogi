<form action="/users/:id/restaurants" method="POST">
  <div>
    <label for="name"> Name </label>
    <input type="text" name="Name">
  </div>
  <div>
    <label for="phone"> Phone Number </label>
    <input type="text" name="Phone Number">
  </div>
  
  <input type="text" name="name" value="Name"><br>
  <input type="text" name="phone" value="Phone Number"><br>
  <input type="text" name="address" value="Address"><br>
  <input type="text" name="city" value="City"><br>
  <input type="text" name="state" value="State"><br>
  <input type="text" name="zip" value="Zip Code"><br>
  <input type="text" name="mfhours" value="Monday - Friday Hours"><br>
  <input type="text" name="sathours" value="Saturday Hours"><br>
  <input type="text" name="sunhours" value="Sunday Hours"><br>
  <input type="text" name="most_popular" value="Popular Dish"><br>
  <input type="text" name="favorite_dish" value="My Favorite Dish"><br>
  <input type="submit" value="Submit Restaurant Info"><br>
</form>
